{
  "name": "v3-admin-vite",
  "version": "4.4.0",
  "description": "一个免费开源的中后台管理系统基础解决方案，基于 Vue3、TypeScript、Element Plus、Pinia 和 Vite 等主流技术",
  "author": {
    "name": "pany",
    "email": "939630029@qq.com",
    "url": "https://github.com/pany-ang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/un-pany/v3-admin-vite.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --mode mock",
    "dev:mock": "vite --mode mock",
    "build:dev": "vite build --mode mock",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@antv/g6": "^4.8.24",
    "@element-plus/icons-vue": "2.3.1",
    "@types/three": "^0.155.0",
    "animate.css": "^4.1.1",
    "axios": "1.6.8",
    "color": "^4.2.3",
    "crypto-js": "^4.2.0",
    "dayjs": "1.11.10",
    "dom-helpers": "^5.2.1",
    "echarts": "^5.5.0",
    "echarts-gl": "^2.0.9",
    "echarts-stat": "^1.2.0",
    "echarts-wordcloud": "^2.1.0",
    "element-plus": "2.6.2",
    "element-resize-detector": "^1.2.4",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.5",
    "insert-css": "^2.0.0",
    "js-cookie": "3.0.5",
    "keymaster": "^1.6.2",
    "lodash-es": "4.17.21",
    "luckyexcel": "^1.0.1",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "resize-observer": "^1.0.4",
    "screenfull": "6.0.2",
    "three": "^0.155.0",
    "vdirs": "^0.1.8",
    "vue": "3.4.21",
    "vue-echarts": "^6.7.2",
    "vue-router": "4.3.0",
    "vue-types": "^5.1.2",
    "vue3-count-to": "^1.1.2",
    "vue3-sketch-ruler": "^1.3.15",
    "vuedraggable": "4.0.1",
    "vxe-table": "4.5.21",
    "vxe-table-plugin-element": "4.0.1",
    "web-storage-cache": "^1.1.1",
    "xe-utils": "3.5.22"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.30",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@types/postcss-pxtorem": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/test-utils": "2.4.5",
    "autoprefixer": "^10.4.17",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.23.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "mockjs": "^1.1.0",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "3.2.5",
    "prettier-eslint": "^16.3.0",
    "sass": "1.72.0",
    "typescript": "5.4.3",
    "unocss": "0.58.6",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "5.2.6",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.4.0",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "2.0.7"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
    "vite-admin",
    "element-plus",
    "element-plus-admin",
    "ts",
    "typescript"
  ],
  "license": "MIT"
}
